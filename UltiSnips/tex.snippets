snippet nl "Adds the newline tag"
\newline
endsnippet

snippet tgraph "Inserts a Tikz graph" b
\tikz \graph [binary tree layout] {
	$0
};
endsnippet

snippet nod "adds a Tikz graph node" b
\node (${1:name}) at (${2:col},${3:line}) [${4:shape}] {${0:content}};
endsnippet

